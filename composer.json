{
    "name": "atiladanvi/cid-api-php",
    "description": "Api com todas as doen√ßas catalogadas pela OMS.",
    "authors": [
        {
            "name": "Atila Silva",
            "email": "atila.danvi@outlook.com"
        }
    ],
    "keywords": ["cid10", "OMS", "WHO"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "fruitcake/laravel-cors": "^2.0",
        "graham-campbell/throttle": "^8.0",
        "laravel/lumen-framework": "^7.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
